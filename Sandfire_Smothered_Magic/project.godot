; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={
}

[application]

config/name="Sandfire_Smothered_Magic"
run/main_scene="res://scenes/Title.tscn"
config/icon="res://textures/main_logo.png"

[autoload]

JsonData="*res://scripts/JSONData.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=1919
window/size/test_height=1079
window/stretch/mode="2d"
window/stretch/aspect="keep"

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

environment/default_clear_color=Color( 0.054902, 0.054902, 0.0509804, 1 )
environment/default_environment="res://Environment.tres"
